{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "ts-node src/main",
    "debug": "node --inspect=0.0.0.0:9229 -r ts-node/register src/main.ts",
    "prestart:prod": "yarn run build",
    "start:prod": "node dist/main.js",
    "test": "jest --config=jest.json --detectOpenHandles",
    "test-e2e": "jest --config=jest-e2e.json --detectOpenHandles",
    "test-single": "jest --config=jest.json src/modules/auth/auth.controller.spec.ts"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "@nest-modules/mailer": "^1.1.3",
    "@nestjs/common": "^6.4.0",
    "@nestjs/core": "^6.4.0",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.4.0",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/typeorm": "^6.1.3",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.2.3",
    "cors": "^2.8.5",
    "mysql": "^2.17.1",
    "nest-logger": "^2.0.1",
    "nestjs-config": "^1.4.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.2",
    "swagger-ui-express": "^4.0.6",
    "tls": "^0.0.1",
    "typeorm": "^0.2.18",
    "typescript": "3.5.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.3.2",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.15",
    "@types/node": "10.14.13",
    "build": "^0.1.4",
    "faker": "^4.1.0",
    "jest": "^24.8.0",
    "run": "^1.4.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "8.2.0",
    "tslint": "5.17.0",
    "yarn": "^1.17.3"
  }
}
